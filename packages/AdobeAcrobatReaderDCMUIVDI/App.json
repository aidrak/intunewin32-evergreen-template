{
  "Application": {
    "Name": "AdobeAcrobatReaderDC",
    "Filter": {
      "Architecture": "x64",
      "Language": "MUI"
    }
  },
  "PackageInformation": {
    "SetupType": "EXE",
    "SetupFile": "AcroRdrDCx64_MUI.exe",
    "Version": "YOURVERSION"
  },
  "Information": {
    "DisplayName": "Adobe Acrobat Reader DC (MUI VDI)",
    "Description": "Adobe Acrobat Reader DC Multi-language edition optimized for VDI environments. Includes disabled updates, browser integration, and thumbnails for optimal performance.",
    "Publisher": "Adobe Inc.",
    "InformationURL": "https://www.adobe.com/acrobat/pdf-reader.html",
    "PrivacyURL": "https://www.adobe.com/privacy.html"
  },
  "Program": {
    "InstallCommand": "powershell.exe -ExecutionPolicy Bypass -WindowStyle Hidden -File .\\Install.ps1",
    "UninstallCommand": "msiexec.exe /uninstall {AC76BA86-1033-FF00-7760-BC15014EA700} /quiet /norestart",
    "InstallExperience": "system",
    "DeviceRestartBehavior": "suppress"
  },
  "RequirementRule": {
    "MinimumRequiredOperatingSystem": "W10_1809",
    "Architecture": "x64"
  },
  "DetectionRule": {
    "Type": "File",
    "Path": "C:\\Program Files\\Adobe\\Acrobat DC\\Acrobat",
    "FileOrFolder": "Acrobat.exe",
    "Check32BitOn64System": false,
    "DetectionType": "Version",
    "Operator": "greaterThanOrEqual",
    "Value": "YOURVERSION"
  }
}
