{
  "Application": {
    "Name": "AdobeAcrobatReaderDC",
    "Filter": {
      "Architecture": "x64",
      "Language": "English"
    }
  },
  "PackageInformation": {
    "SetupType": "EXE",
    "SetupFile": "AcroRdrDCx64_en_US.exe",
    "Version": "YOURVERSION"
  },
  "Information": {
    "DisplayName": "Adobe Acrobat Reader DC",
    "Description": "Adobe Acrobat Reader DC is the free, trusted global standard for viewing, printing, e-signing, sharing, and annotating PDFs.",
    "Publisher": "Adobe Inc.",
    "InformationURL": "https://www.adobe.com/acrobat/pdf-reader.html",
    "PrivacyURL": "https://www.adobe.com/privacy.html"
  },
  "Program": {
    "InstallCommand": "powershell.exe -ExecutionPolicy Bypass -WindowStyle Hidden -File .\\Install.ps1",
    "UninstallCommand": "msiexec.exe /uninstall {AC76BA86-7AD7-1033-7B44-AC0F074E4100} /quiet /norestart",
    "InstallExperience": "system",
    "DeviceRestartBehavior": "suppress"
  },
  "RequirementRule": {
    "MinimumRequiredOperatingSystem": "W10_1809",
    "Architecture": "x64"
  },
  "DetectionRule": {
    "Type": "File",
    "Path": "C:\\Program Files\\Adobe\\Acrobat DC\\Acrobat",
    "FileOrFolder": "Acrobat.exe",
    "Check32BitOn64System": false,
    "DetectionType": "Version",
    "Operator": "greaterThanOrEqual",
    "Value": "YOURVERSION"
  }
}
